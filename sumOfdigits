import java.util.Scanner;
public class Main{
    static int sumOfdigits(int n){
        int sum = 0;
        while(n>0){
            int digit = n % 10;
            sum = sum+digit;
            n = n / 10;
        }
        return sum;
    }
    public static void main(String args[]){
        System.out.print(" n = ");
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        System.out.println(sumOfdigits(n));
    }
}
