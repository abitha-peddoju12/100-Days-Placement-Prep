import java.util.Scanner;
public class Main {
    static boolean PrefectNumber(int n){
        int sum = 0;
        for(int i = 1; i < n; i++){
            if(n % i == 0){
                sum = sum + i;
            }
        }
        return sum == n;
    }
    public static void main(String[] args){
        System.out.print("n = ");
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        if(PrefectNumber(n)){
            System.out.println("PrefectNumber");
        }else {
            System.out.println("Not a PrefectNumber");
        }
    }
    
}
