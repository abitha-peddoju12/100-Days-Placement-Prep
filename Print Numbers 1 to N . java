import java.util.Scanner;
public class Main{
    static void  print(int n){
        for(int i=1; i<=n; i++){
            System.out.print(i + " ");
        }
    }
    public static void main(String[] args){
        System.out.print("n = ");
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        print(n);
        
    }
}
