import java.util.Scanner;
public class Main {
    static int countDigitLessThanx(int n ,int x){
        int count = 0;
        while(n>0){
            int digit = n % 10;
            if(digit < x){
                count++;
        }
         n = n / 10;
    }
      return count;
}
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int x = sc.nextInt();
        System.out.println(countDigitLessThanx(n,x));
    }
    
}
